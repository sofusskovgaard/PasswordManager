@page
@using PasswordManager.Data.Entities.User
@model IndexModel
@{
    ViewData["Title"] = "Welcome";
}

<div>
    @if (Model.LoggedIn)
    {
        <div class="d-flex">
            <div class="w-25 mr-5">
                @await Html.PartialAsync("Partials/Menu")
            </div>
            <div class="w-75">
                <p>Uhh, suuuh duuu</p>
            </div>
        </div>
    }
    else
    {
        <p>There's no logged in user.
            <span>
                <a asp-page="Auth/Login">Login</a>
                or
                <a asp-page="Auth/Register">Register</a>
            </span>
        </p>
    }
</div>